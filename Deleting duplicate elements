# Python code to remove duplicate elements
def Delete(duplicate):
    final_list = []
    for number in duplicate:
        if number not in final_list:
            final_list.append(number)
    return final_list


# printing res:
duplicate = [2, 4, 10, 20, 5, 2, 20, 4]
print(Delete(duplicate))

